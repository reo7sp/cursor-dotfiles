{
  "http.proxy": "http://127.0.0.1:1087",
  "cursor.composer.shouldAutoAcceptDiffs": false,
  "editor.fontFamily": "Jetbrains Mono",
  "editor.fontLigatures": false,
  "editor.fontSize": 14,
  "editor.fontVariations": false,
  "editor.formatOnPaste": true,
  "editor.inlineSuggest.enabled": false,
  "editor.inlineSuggest.syntaxHighlightingEnabled": false,
  "editor.letterSpacing": -0.4,
  "editor.lineHeight": 18,
  "editor.minimap.showSlider": "always",
  "editor.mouseWheelScrollSensitivity": 0,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.snippetSuggestions": "top",
  "editor.wordWrap": "off",
  "explorer.confirmDelete": false,
  "explorer.decorations.badges": false,
  "explorer.decorations.colors": false,
  "files.autoSave": "afterDelay",
  "files.insertFinalNewline": true,
  "git.openRepositoryInParentFolders": "always",
  "outline.problems.badges": false,
  "outline.problems.colors": false,
  "outline.problems.enabled": false,
  "outline.showArrays": false,
  "outline.showBooleans": false,
  "outline.showConstants": false,
  "outline.showEnumMembers": false,
  "outline.showFields": false,
  "outline.showFiles": false,
  "outline.showFunctions": false,
  "outline.showKeys": false,
  "outline.showNull": false,
  "outline.showNumbers": false,
  "outline.showObjects": false,
  "outline.showProperties": false,
  "outline.showStrings": false,
  "outline.showVariables": false,
  "problems.decorations.enabled": false,
  "python.defaultInterpreterPath": "/Users/o.morozenkov/Documents/code/_mailru/biz/venv",
  "python.languageServer": "None",
  "redhat.telemetry.enabled": false,
  "terminal.integrated.suggest.suggestOnTriggerCharacters": false,
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-,>": false,
    "<C-.>": false,
    "<C-1>": false,
    "<C-2>": false,
    "<C-3>": false,
    "<C-4>": false,
    "<C-5>": false,
    "<C-6>": false,
    "<C-7>": false,
    "<C-8>": false,
    "<C-9>": false,
    "<C-BS>": true,
    "<C-Down>": true,
    "<C-Left>": true,
    "<C-Right>": true,
    "<C-Up>": true,
    "<C-[>": true,
    "<C-]>": true,
    "<C-q>": true,
    "<C-w>": true
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    { "before": [ "K" ], "commands": [ "editor.action.showHover" ] },
    { "before": [ "g", "d" ], "commands": [ "editor.action.revealDefinition" ] },
    { "before": [ "g", "D" ], "commands": [ "editor.action.goToImplementation" ] },
    { "before": [ "g", "y" ], "commands": [ "editor.action.goToTypeDefinition" ] },
    { "before": [ "g", "e" ], "commands": [ "editor.action.referenceSearch.trigger" ] },
    { "before": [ "g", "Q" ], "commands": [ "editor.action.formatDocument" ] },
    { "before": [ "g", "R" ], "commands": [ "editor.action.refactor" ] },
    { "before": [ "g", "r" ], "commands": [ "editor.action.rename" ] },
    { "before": [ "g", "s" ], "commands": [ "workbench.action.findInFiles" ] },
    { "before": [ "<leader>", "f" ], "commands": [ "workbench.action.quickOpen" ] },
    { "before": [ "<leader>", "s" ], "commands": [ "workbench.action.findInFiles" ] },
    { "before": [ "<leader>", "e" ], "commands": [ "workbench.action.openRecent" ] },
    { "before": [ "<leader>", "b" ], "commands": [ "workbench.action.showAllEditors" ] },
    { "before": [ "<leader>", "k" ], "commands": [ "workbench.action.gotoSymbol" ] },
    { "before": [ "<leader>", "d" ], "commands": [ "workbench.action.problems.focus" ] },
    { "before": [ "<leader>", "t" ], "commands": [ "workbench.view.explorer", "workbench.files.action.focusFilesExplorer" ] },
    { "before": [ "<leader>", "o" ], "commands": [ "outline.focus" ] },
    { "before": [ "-" ], "commands": [ "workbench.files.action.showActiveFileInExplorer" ] },
    { "before": [ "_" ], "commands": [ "workbench.files.action.showActiveFileInExplorer" ] },
    { "before": [ "<C-BS>" ], "commands": [ "workbench.action.openPreviousRecentlyUsedEditorInGroup" ] },
    { "before": [ "<C-[>" ], "commands": [ "workbench.action.previousEditor" ] },
    { "before": [ "<C-]>" ], "commands": [ "workbench.action.nextEditor" ] },
    { "before": [ "<C-1>" ], "commands": [ "workbench.action.openEditorAtIndex1" ] },
    { "before": [ "<C-2>" ], "commands": [ "workbench.action.openEditorAtIndex2" ] },
    { "before": [ "<C-3>" ], "commands": [ "workbench.action.openEditorAtIndex3" ] },
    { "before": [ "<C-4>" ], "commands": [ "workbench.action.openEditorAtIndex4" ] },
    { "before": [ "<C-5>" ], "commands": [ "workbench.action.openEditorAtIndex5" ] },
    { "before": [ "<C-6>" ], "commands": [ "workbench.action.openEditorAtIndex6" ] },
    { "before": [ "<C-7>" ], "commands": [ "workbench.action.openEditorAtIndex7" ] },
    { "before": [ "<C-8>" ], "commands": [ "workbench.action.openEditorAtIndex8" ] },
    { "before": [ "<C-9>" ], "commands": [ "workbench.action.openEditorAtIndex9" ] },
    { "before": [ "<C-,>" ], "commands": [ "workbench.action.moveEditorLeftInGroup" ] },
    { "before": [ "<C-.>" ], "commands": [ "workbench.action.moveEditorRightInGroup" ] },
    { "before": [ "<C-q>" ], "commands": [ "workbench.action.closeActiveEditor" ] },
    { "before": [ "<C-w>", "Q" ], "commands": [ "workbench.action.closeActiveEditor" ] },
    { "before": [ "<C-w>", "O" ], "commands": [ "workbench.action.closeOtherEditors" ] },
    { "before": [ "<C-Up>" ], "commands": [ "workbench.action.increaseViewSize" ] },
    { "before": [ "<C-Down>" ], "commands": [ "workbench.action.decreaseViewSize" ] },
    { "before": [ "<C-Left>" ], "commands": [ "workbench.action.decreaseViewSize" ] },
    { "before": [ "<C-Right>" ], "commands": [ "workbench.action.increaseViewSize" ] }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    { "before": [ "g", "R" ], "commands": [ "editor.action.refactor" ] },
    { "before": [ "<leader>", "s" ], "commands": [ "workbench.action.findInFiles" ] },
    { "before": [ "g", "s" ], "commands": [ "workbench.action.findInFiles" ] }
  ],
  "vim.highlightedyank.enable": true,
  "vim.showMarksInGutter": true,
  "vim.smartRelativeLine": true,
  "vim.sneak": true,
  "vim.sneakReplacesF": true,
  "vim.sneakUseIgnorecaseAndSmartcase": true,
  "vim.targets.enable": true,
  "window.closeWhenEmpty": false,
  "window.commandCenter": true,
  "window.restoreWindows": "none",
  "window.title": "${activeEditorMedium}",
  "window.zoomLevel": 0,
  "breadcrumbs.enabled": true,
  "breadcrumbs.filePath": "on",
  "workbench.colorTheme": "Catppuccin Latte",
  "workbench.editor.decorations.badges": false,
  "workbench.editor.decorations.colors": false,
  "workbench.iconTheme": "catppuccin-latte",
  "workbench.reduceMotion": "on",
  "catppuccin.bracketMode": "monochromatic",
  "catppuccin.extraBordersEnabled": true,
  "catppuccin.italicKeywords": false,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "editor.tabSize": 4,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.rulers": [80, 120],
  "editor.lineNumbers": "relative",
  "editor.renderWhitespace": "selection",
  "editor.glyphMargin": true,
  "search.smartCase": true,
  "vim.hlsearch": true,
  "vim.ignorecase": true,
  "vim.smartcase": true,
  "workbench.editor.openSideBySideDirection": "right",
  "vim.enableNeovim": true,
  "viml.highVimHelpIndentStylePriority": false,
  "makefile.configureOnOpen": true
}
