{
    "window.commandCenter": true,
    "workbench.colorTheme": "Catppuccin Latte",
    "window.closeWhenEmpty": false,
    "files.insertFinalNewline": true,
    "editor.fontFamily": "Jetbrains Mono",
    "editor.fontSize": 13.5,
    "files.autoSave": "onFocusChange",
    "editor.mouseWheelScrollSensitivity": 0,
    "window.title": "${activeEditorFull}${separator}${rootName}",
    "editor.wordWrap": "off",
    "editor.formatOnPaste": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.snippetSuggestions": "top",
    "window.restoreWindows": "none",
    "explorer.confirmDelete": false,
    "python.languageServer": "None",
    "editor.minimap.showSlider": "always",
    "vim.highlightedyank.enable": true,
    "vim.showMarksInGutter": true,
    "vim.targets.enable": true,
    "vim.sneak": true,
    "vim.sneakReplacesF": true,
    "vim.sneakUseIgnorecaseAndSmartcase": true,
    "vim.smartRelativeLine": true,
    "outline.showArrays": false,
    "outline.showBooleans": false,
    "outline.showVariables": false,
    "outline.showNumbers": false,
    "outline.showNull": false,
    "outline.showProperties": false,
    "outline.showStrings": false,
    "workbench.iconTheme": "catppuccin-latte",
    "catppuccin.bracketMode": "neovim",
    "catppuccin.italicKeywords": false,
    "workbench.reduceMotion": "on",
    "explorer.decorations.badges": false,
    "explorer.decorations.colors": false,
    "outline.problems.enabled": false,
    "problems.decorations.enabled": false,
    "workbench.editor.decorations.badges": false,
    "workbench.editor.decorations.colors": false,
    "cursor.composer.shouldAutoAcceptDiffs": false
}
